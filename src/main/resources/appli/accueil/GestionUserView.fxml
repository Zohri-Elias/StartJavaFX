<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="appli.acceuil.GestionUserController"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <TableView fx:id="tableauUser" onMouseClicked="#cliqueTableauEvent">
            <!-- Les colonnes sont ajoutées dynamiquement dans le contrôleur -->
        </TableView>

        <Button fx:id="btnSupprimer" text="Supprimer" onAction="#supprimerUtilisateur" disable="true"/>
    </children>
</AnchorPane>
