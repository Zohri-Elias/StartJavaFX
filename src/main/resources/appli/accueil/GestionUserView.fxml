    <children>
        <TableView fx:id="tableauUser" onMouseClicked="#cliqueTableauEvent">
            <!-- Les colonnes sont ajoutées dynamiquement dans le contrôleur -->
        </TableView>

        <Button layoutX="282.0" layoutY="80.0" onAction="#btnRetour" text="Retour" />
        <Button fx:id="btnSupprimer" disable="true" layoutX="256.0" layoutY="361.0" onAction="#supprimerUtilisateur" text="Supprimer" />
    </children>
</AnchorPane>
